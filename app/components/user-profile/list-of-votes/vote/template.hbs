<div class="vote-wrapper" data-test-single-vote-in-list >
  <div class="information-wrapper">
    <h6>Idea title:
      <span>
        <LinkTo @route="idea-details" @model={{this.idea.id}}>
          {{this.idea.title}}
        </LinkTo>
      </span>
    </h6>
  </div>
  <div class="information-wrapper">
    <h6>Date:<span>{{this.voteDate}}</span></h6>
  </div>
  <div class="information-wrapper">
    <h6>Number of kudos:<span data-test-single-vote-number-of-kudos>{{@vote.numberOfVotes}}</span></h6>
  </div>
</div>